{
  "Resources": {
    "SomeGroup": {
      "Type": "AWS::IAM::Group"
    },
    "NotResourcePolicy": {
      "Type": "AWS::IAM::Policy",
      "Properties": {
        "PolicyName": "somePolicy",
        "PolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Action": "rds:CreateDBInstance",
              "NotResource": {
                "Fn::Join": [
                  "",
                  [
                    "arn:aws:rds:",
                    {
                      "Ref": "AWS::Region"
                    },
                    ":",
                    {
                      "Ref": "AWS::AccountId"
                    },
                    ":db:test*"
                  ]
                ]
              },
              "Condition": {
                "StringEquals": {
                  "rds:DatabaseEngine": "mysql"
                }
              }
            }
          ]
        },
        "Groups": [
          {
            "Ref": "SomeGroup"
          }
        ]
      }
    }
  }
}